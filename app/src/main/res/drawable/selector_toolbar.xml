<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <stroke android:width="1dip" android:color="#296397" />
            <gradient android:angle="-90" android:startColor="#296397" android:endColor="#296397"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <stroke android:width="1dip" android:color="#296397" />
            <solid android:color="#296397"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <stroke android:width="1dip" android:color="#12528c" />
            <gradient android:angle="-90" android:startColor="#12528c" android:endColor="#12528c" />
        </shape>
    </item>
</selector>